<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>AdvisorController</servlet-name>
		<display-name>AdvisorController</display-name>
		<description></description>
		<servlet-class>com.wealthServlet.AdvisorController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CustomerController</servlet-name>
		<display-name>CustomerController</display-name>
		<description></description>
		<servlet-class>com.wealthServlet.CustomerController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AdvisorController</servlet-name>
		<url-pattern>/AdvisorController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AdvisorController</servlet-name>
		<url-pattern>/AdvisorController/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CustomerController</servlet-name>
		<url-pattern>/CustomerController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CustomerController</servlet-name>
		<url-pattern>/CustomerController/*</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
